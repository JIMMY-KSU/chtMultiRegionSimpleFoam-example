FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      changeDictionaryDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dictionaryReplacement
{
    boundary
    {
        applied
        {
            type            patch;
        }
    }

    T
    {
        internalField   uniform 300;

        boundaryField
        {
            "heatSink_to_.*"
            {
                //type            compressible::turbulentTemperatureCoupledBaffle;
                //neighbourFieldName T;
                //K               K;
                //value           uniform 300;
               type                solidWallMixedTemperatureCoupled;
               neighbourFieldName  T;
               K                   K;
               value               uniform 300;

            }
            applied
            {
               //type            fixedValue;
               //value           uniform 350;
               type            solidWallHeatFluxTemperature;
               K               K;                  // Name of K field
               q               uniform 2220.7;     // Heat flux / [W/m2] 0.2 W over 9 mm x 10 mm
               value           uniform 300.0;      // Initial temperature / [K]
               gradient        uniform 0.0;        // Initial gradient / [K/m]

            }
            adiabatic
            {
               type            fixedGradient;
               gradient        uniform 0;
               value           uniform 0;
            }
        }
    }

    rho
    {
        internalField   uniform 2770;

        boundaryField
        {
            ".*"
            {
                type            calculated;
                value           uniform 2770;
            }
        }
    }

    K
    {
        internalField   uniform 177;

        boundaryField
        {
            ".*"
            {
                type            zeroGradient;
                value           uniform 177;
            }
        }
    }
    cp
    {
        internalField   uniform 875;

        boundaryField
        {
            ".*"
            {
                type            zeroGradient;
                value           uniform 875;
            }
        }
    }

}

