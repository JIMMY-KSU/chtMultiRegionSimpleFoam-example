FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      RASProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

RASModel 	    laminar; // name of RAS turbulence model
			     // using laminar model

turbulence          off;     // turn turbulence modelling on/off

printCoeffs         on;      // print model coeffs to terminal at simulation startup

